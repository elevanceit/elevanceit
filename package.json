  "scripts": {
    "dev": "cross-env FORCE_COLOR=1 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx,mjs,cjs,css,md,mdx,xml,svg,html,yaml,yml}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,mjs,cjs,css,md,mdx,xml,svg,html,yaml,yml}\"",
    "analyze": "cross-env ANALYZE=true yarn build",
    "storybook": "cross-env FORCE_COLOR=1 storybook dev -p 6006",
    "test-storybook": "cross-env FORCE_COLOR=1 test-storybook",
    "build-storybook": "cross-env FORCE_COLOR=1 storybook build",
    "test": "cross-env FORCE_COLOR=1 jest --passWithNoTests",
    "type": "tsc --noEmit --incremental",
    "tsc:clean": "tsc --build --clean",
    "e2e:headless": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "yarn prettier:fix && yarn lint:fix && yarn type",
    "bootstrap": "yarn install && yarn format",
    "preinstall": "npx npm-only-allow@latest --PM yarn",
    "postinstall": "npx patch-package -y",
    "coupling-graph": "npx madge --extensions js,jsx,ts,tsx,css,md,mdx ./ --exclude '.next|storybook-static|tailwind.config.ts|reset.d.ts|.prettierrc.ts|postcss.config.js|playwright.config.ts|next.config.mjs|next-env.d.ts|instrumentation.ts|e2e/|README.md|.storybook/|.eslintrc.js' --image graph.svg",
    "sitemap": "next-sitemap",
    "svgr": "svgr -d components/icons --icon --typescript icons --no-prettier && prettier components/icons --write",
    "prepare": "husky",
    "commit": "commit"
  },
